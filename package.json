{
  "name": "deciduously-com",
  "private": true,
  "version": "0.0.1",
  "description": "Personal blog, portfolio, and CV",
  "scripts": {
    "build": "NODE_ENV=production stencil build --prerender",
    "lint": "run-p lint:ts lint:styles",
    "lint:styles": "stylelint src/**/*.css",
    "lint:ts": "eslint src/**/*.ts*",
    "start": "stencil build --dev --watch --serve",
    "stylelint:fix": "stylelint --fix src/**/*.css",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^1.8.5",
    "@stencil/router": "^1.0.1",
    "markdown-it": "^10.0.0",
    "tailwindcss": "^1.1.4"
  },
  "license": "MIT",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.0.3",
    "@stencil/eslint-plugin": "^0.2.1",
    "@stencil/postcss": "^1.0.1",
    "@types/autoprefixer": "^9.6.1",
    "@types/cssnano": "^4.0.0",
    "@types/jest": "24.0.25",
    "@types/prettier": "^1.19.0",
    "@types/puppeteer": "1.20.3",
    "@types/stylelint": "^9.10.1",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-react": "^7.18.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "puppeteer": "1.20.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "strict-tslint-config": "^1.0.1",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
